{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o public/dist/ --output-style compressed",
    "watch-css": "node-sass-chokidar --include-path ./node_modules src/ -o public/dist/ --watch --output-style compressed",
    "start": "node bin/www",
    "dev": "concurrently \"npm run watch-css\" \"nodemon bin/www\" \"webpack --watch\"",
    "prd": "pm2 start bin/www",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "cloudinary-core": "^2.5.0",
    "debug": "^2.6.3",
    "ekko-lightbox": "^5.3.0",
    "jquery": "^3.3.1",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-helmet": "^4.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-minify": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "materialize-css": "^1.0.0-rc.2",
    "popper.js": "^1.14.4",
    "pug": "^2.0.0-rc.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "concurrently": "^4.0.1",
    "expose-loader": "^0.7.5",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.8.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
